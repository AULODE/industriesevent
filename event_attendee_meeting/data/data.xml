<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate="1">
		<record id="attendee_meeting_email_template" model="mail.template">
			<field name="lang">${object.lang}</field>
			<field name="name">Attendee Meeting</field>
			<field name="subject">${object.sender_id.name} wants to meet you</field>
			<field name="email_from">${object.sender_id.email}</field>
			<field name="email_to">${object.partner_id.email}</field>
			<field name="model_id" ref="event_attendee_meeting.model_attendee_meeting" />
			<field name="auto_delete" eval="True" />
			<field name="body_html">
				
			</field>
		</record>

		<record id="meeting_done_email_template" model="mail.template">
			<field name="lang">${object.lang}</field>
			<field name="name">Attendee Meeting</field>
			<field name="model_id" ref="calendar.model_calendar_event" />
			<field name="auto_delete" eval="True" />
			<field name="body_html">
				
			</field>
		</record>

	</data>
</openerp>

